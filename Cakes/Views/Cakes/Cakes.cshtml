
@{
    Layout = "_Layout";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>All sweet</title>
</head>
<body>
    <h1>@ViewBag.Title</h1>
    <div class="row mt-5 mb-2">
        @{
            foreach (Cake cake in Model.GetCakes)
            {
                @await Html.PartialAsync("_CakeCard", cake)
            }
        }
    </div>
</body>
</html>


